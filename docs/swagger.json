{
  "openapi":"3.0.1",
  "info":{
    "description":"This service transfer your picture into a master piece style",
    "title":"Neural Style",
    "version":"1.0.0"
  },
  "tags":[
    {
      "name":"process",
      "description":"transfer a picture into a master piece style"
    }
  ],
  "paths":{
    "/process":{
      "post":{
        "security":[
          {
            "apikey":[

            ]
          }
        ],
        "tags":[
          "process"
        ],
        "summary":"Will transfer your picture into a master piece style",
        "description":"This service will transfer your picture into a master piece style",
        "operationId":"process",
        "responses":{
          "400":{
            "description":"Error"
          },
          "401":{
            "description":"Unauthorized"
          },
          "200":{
            "content":{
              "image/png":{
                "schema":{
                  "type":"string",
                  "format":"binary",
                  "items":{
                    "$ref":"#/components/schemas/Response"
                  }
                }
              },
              "application/octet-stream":{
                "schema":{
                  "type":"string",
                  "format":"binary",
                  "items":{
                    "$ref":"#/components/schemas/Response"
                  }
                }
              }
            }
          }
        },
        "requestBody":{
          "description":"Will blur the faces detected in the provided picture",
          "required":true,
          "content":{
            "application/json":{
              "schema":{
                "$ref":"#/components/schemas/Body"
              }
            }
          }
        }
      }
    }
  },
  "components":{
    "securitySchemes":{
      "apikey":{
        "type":"apiKey",
        "name":"X-OVH-Api-Key",
        "in":"header"
      }
    },
    "schemas":{
      "Body":{
        "type":"object",
        "required":[
          "url",
	  "style"
        ],
        "properties":{
          "url":{
            "type":"string",
            "example":"https://i.ibb.co/BGHyHjc/input.png"
          },
	  "style":{
            "type":"string",
	    "example":"van-gogh",
	    "description": "painter style to transfer to authorized values among the followings : kandinsky  beret-picasso  seated-nude  shipwreck  starry-night  the_scream  woman-with-hat-matisse"
	  }
        },
        "Response": {
          "description": "Picture with master piece style",
          "content":{
            "image/png":{
              "schema":{
                "type":"string",
                "format":"binary"
              }
            }
          }
        }
      }
    }
  }
}
